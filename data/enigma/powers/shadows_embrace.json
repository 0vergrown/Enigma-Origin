{
	"name":"Shadow's Embrace",
	"description":"With the shadows as your ally, you can appear behind anyone, unseen and unheard.",
	"type":"origins:active_self",
	"entity_action":{
	   "type":"origins:raycast",
	   "distance":64,
	   "block":false,
	   "entity":true,
	   "shape_type":"visual",
	   "fluid_handling":"none",
	   "bientity_condition":{
		  "type":"origins:target_condition",
		  "condition":{
			 "type":"origins:living"
		  }
	   },
	   "bientity_action":{
		  "type":"origins:target_action",
		  "action":{
			 "type":"origins:and",
			 "actions":[
				{
				   "type":"origins:execute_command",
				   "command":"tag @s add enigma.shadows_embrace_target"
				},
				{
				   "type":"origins:grant_power",
				   "power":"enigma:shadows_embrace_target_glow",
				   "source":"enigma:shadows_embrace_target_glow"
				},
				{
				   "type":"origins:delay",
				   "action":{
					  "type":"origins:and",
					  "actions":[
						 {
							"type":"origins:revoke_power",
							"power":"enigma:shadows_embrace_target_glow",
							"source":"enigma:shadows_embrace_target_glow"
						 },
						 {
							"type":"origins:execute_command",
							"command":"tag @s remove enigma.shadows_embrace_target"
						 }
					  ]
				   },
				   "ticks":62
				}
			 ]
		  }
	   },
	   "hit_action":{
		  "type":"origins:and",
		  "actions":[
			 {
				"type":"origins:play_sound",
				"sound":"minecraft:entity.experience_orb.pickup",
				"volume":1.00,
				"pitch":1.00
			 },
			 {
				"type":"origins:apply_effect",
				"effects":[
				   {
					  "effect":"minecraft:blindness",
					  "duration":80,
					  "amplifier":0,
					  "is_ambient":false,
					  "show_particles":false,
					  "show_icon":false
				   },
				   {
					  "effect":"minecraft:weakness",
					  "duration":60,
					  "amplifier":2,
					  "is_ambient":false,
					  "show_particles":false,
					  "show_icon":false
				   },
				   {
					  "effect":"minecraft:slowness",
					  "duration":60,
					  "amplifier":10,
					  "is_ambient":false,
					  "show_particles":false,
					  "show_icon":false
				   }
				]
			 },
			 {
				"type":"origins:delay",
				"action":{
				   "type":"origins:play_sound",
				   "sound":"minecraft:block.bell.resonate",
				   "volume":1.00,
				   "pitch":1.00
				},
				"ticks":1
			 },
			 {
				"type":"origins:delay",
				"action":{
				   "type":"origins:play_sound",
				   "sound":"minecraft:block.bell.resonate",
				   "volume":1.00,
				   "pitch":1.00
				},
				"ticks":5
			 },
			 {
				"type":"origins:delay",
				"action":{
				   "type":"origins:play_sound",
				   "sound":"minecraft:block.bell.resonate",
				   "volume":1.00,
				   "pitch":1.00
				},
				"ticks":10
			 },
			 {
				"type":"origins:delay",
				"action":{
				   "type":"origins:spawn_particles",
				   "particle":"minecraft:enchant",
				   "count":300,
				   "speed":1,
				   "force":true,
				   "spread":{
					  "x":0.1,
					  "y":0.1,
					  "z":0.1
				   },
				   "offset_y":1.5
				},
				"ticks":25
			 },
			 {
				"type":"origins:delay",
				"action":{
				   "type":"origins:execute_command",
				   "command":"tp @s @e[limit=1,tag=enigma.shadows_embrace_target,sort=nearest]"
				},
				"ticks":60
			 },
			 {
				"type":"origins:delay",
				"action":{
				   "type":"origins:and",
				   "actions":[
					  {
						 "type":"origins:execute_command",
						 "command":"tp @s ^ ^1.1 ^-1.5 facing entity @e[limit=1,tag=enigma.shadows_embrace_target,sort=nearest]"
					  },
					  {
						 "type":"origins:play_sound",
						 "sound":"minecraft:block.bell.use",
						 "volume":1.00,
						 "pitch":0.50
					  }
				   ]
				},
				"ticks":61
			 }
		  ]
	   },
	   "miss_action":{
		  "type":"origins:and",
		  "actions":[
			 {
				"type":"origins:execute_command",
				"command":"title @s actionbar \"No target in range...\""
			 },
			 {
				"type":"origins:play_sound",
				"sound":"minecraft:block.anvil.place",
				"volume":1.00,
				"pitch":1.00
			 }
		  ]
	   },
	   "command_step":0.25,
	   "command_along_ray_only_on_hit":false
	},
	"cooldown":200,
	"hud_render":{
	   "should_render":true,
	   "sprite_location":"origins:textures/gui/community/spiderkolo/resource_bar_02.png",
	   "bar_index":19
	},
	"key":{
	   "key":"key.origins.primary_active",
	   "continuous":false
	}
}